{
  "version": 2,
  "installCommand": "git submodule update --init --recursive && pnpm install",
  "routes": [
    {
      "src": "/p/(.*)",
      "dest": "/api/p.ts"
    },
    {
      "src": "/d/(.*)",
      "dest": "/api/p.ts"
    },
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.ts"
    },
    {
      "src": "/api/fs/(.*)",
      "dest": "/api/fs.ts"
    },
    {
      "src": "/api/public/(.*)",
      "dest": "/api/public.ts"
    },
    {
      "src": "^/api/(.*)$",
      "dest": "/api/$1"
    },
    {
      "src": "/assets/(.*)",
      "check": true
    },
    {
      "src": "/static/(.*)",
      "check": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
